CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE UUID(UNIQUEIDENTIFIER VARCHAR(36) NOT NULL PRIMARY KEY)
CREATE MEMORY TABLE BOARDS(BOARDID VARCHAR(36) NOT NULL PRIMARY KEY,TITLE VARCHAR(200))
CREATE MEMORY TABLE SECTIONS(SECTIONID VARCHAR(36) NOT NULL PRIMARY KEY,BOARDID VARCHAR(36) NOT NULL,TITLE VARCHAR(200),SORT_ORDER INTEGER,ISWIP BOOLEAN,CONSTRAINT SYS_FK_55 FOREIGN KEY(BOARDID) REFERENCES BOARDS(BOARDID) ON DELETE CASCADE)
CREATE MEMORY TABLE TASKS(TASKID VARCHAR(36) NOT NULL PRIMARY KEY,SECTIONID VARCHAR(36) NOT NULL,TITLE VARCHAR(200),DUEDATE TIMESTAMP,DETAILS VARCHAR(200),COMMENTS VARCHAR(200),PRIORITY INTEGER,STARTINGTIME TIMESTAMP,RUNNINGTIME BIGINT,RUNNING BOOLEAN,CONSTRAINT SYS_FK_60 FOREIGN KEY(SECTIONID) REFERENCES SECTIONS(SECTIONID) ON DELETE CASCADE)
CREATE MEMORY TABLE TASKTAG(TASKID VARCHAR(36) NOT NULL,TAGID VARCHAR(36) NOT NULL,TAG VARCHAR(200),CONSTRAINT SYS_FK_65 FOREIGN KEY(TASKID) REFERENCES TASKS(TASKID) ON DELETE CASCADE)
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 10
SET SCHEMA PUBLIC
